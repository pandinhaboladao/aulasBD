insert into funcionario (nome, cadastro) values ("vitor", "68736211");

insert into venda (codigo_de_venda, produto, nota_fiscal) values ("8923", "90009", "278128321");
insert into venda (codigo_de_venda, produto, nota_fiscal) values ("8923", "90009", "278128321");
insert into venda (codigo_de_venda, produto, nota_fiscal) values ("8923", "90009", "278128321");

